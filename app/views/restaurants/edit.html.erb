<div class="container">
  <h1>Edit this restaurant</h1>
  <%= simple_form_for @restaurant do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.input :category %>
    <%= f.button :submit %>
  <% end %>
  <%= link_to "Back", restaurants_path, class: "btn btn-primary" %>
</div>
